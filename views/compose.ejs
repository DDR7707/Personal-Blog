<%- include("partials/header") %>

<h1>Compose</h1>


  <form action="/Compose" method="post" autocomplete="off">
    <div class="form-group">
      <label style="margin:1.5rem auto">Title</label>
      <input class="form-control" type="text" name="title">
    </div>
    <div class="form-group">
      <label style="margin:1.5rem auto">Post</label>
      <textarea class="form-control" name="content" rows="8" cols="80"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
  </form>



<%- include("partials/footer") %>
